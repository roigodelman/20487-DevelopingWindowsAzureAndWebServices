<?xml version="1.0"?>
<configuration>
  <startup>    
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/></startup>
  
  <system.serviceModel>
    <bindings>
      <wsHttpBinding>
        <binding name="WSHttpBinding_ICalc">
        </binding>
      </wsHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8082/CalcService" binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_ICalc" contract="Service.ICalc" name="WSHttpBinding_ICalc">
        <identity>
          <certificateReference findValue="DemoCert" storeLocation="LocalMachine" storeName="My" x509FindType="FindBySubjectName"/>
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>
